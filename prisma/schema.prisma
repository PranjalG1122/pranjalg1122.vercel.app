datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id             Int       @id @default(autoincrement())
  name           String?   @db.VarChar(255)
  date           DateTime? @default(now())
  guestbookentry String    @db.VarChar(255)
}
